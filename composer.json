{
    "name": "tbessenreither/feature-flag-service-client",
    "description": "A Symfony bundle providing a client for feature flag service",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Bessenreither"
        }
    ],
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/cache": "^7.3",
        "symfony/http-client": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/phpunit-bridge": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Tbessenreither\\FeatureFlagServiceClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tbessenreither\\FeatureFlagServiceClient\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "8.4.0"
        }
    }
}